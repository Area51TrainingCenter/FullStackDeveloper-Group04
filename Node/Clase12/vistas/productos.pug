extends base

include mixins/_formulario

block contenido
	div.container
		div.row
			h1.col.s12 #{tituloPagina.toUpperCase()}

		div.card.col.s8.push-s2
			div.card-content
				table
					thead
						tr
							th ID
							th Producto
					tbody
						if lista.length > 5
							each item in lista
								tr
									td= item.id
									td= item.nombre
						else
							p 
							| Solo hay 5 รณ menos productos
				+formulario()